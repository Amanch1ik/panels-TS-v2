  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.17.9",
    "antd": "^5.12.8",
    "axios": "^1.6.5",
    "dayjs": "^1.11.10",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-leaflet-markercluster": "^5.0.0-rc.0",
